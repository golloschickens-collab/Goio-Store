{
  "name": "goio-store",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "npm install && pip install -r requirements.txt",
    "start": "node start.js",
    "start-empire": "node start.js",
    "start:dev": "node agents/supervisor.js",
    "start:prod": "cross-env SHOPIFY_ENV=prod node agents/supervisor.js",
    "dev": "npx @shopify/cli@latest shopify theme dev --port 9393",
    "dev:prod": "npx @shopify/cli@latest shopify theme dev --store $SHOP_PROD --port 9494",
    "push": "npx @shopify/cli@latest theme push --allow-live",
    "finder": "node scripts/product-finder.js",
    "pricing": "node scripts/pricing-engine.js",
    "seed": "node scripts/seed-products.js",
    "abandoned": "node scripts/abandoned-checkouts.js",
    "postventa": "node scripts/post-sale.js",
    "webhooks": "node scripts/register-webhooks.js",
    "check:dev": "npx cross-env SHOPIFY_ENV=dev node scripts/check-shopify-connection.js",
    "diag:prod": "cross-env SHOPIFY_ENV=prod node scripts/diag-auth.js",
    "products:list": "cross-env SHOPIFY_ENV=prod node scripts/list-products.js",
    "webhooks:dev": "npx cross-env SHOPIFY_ENV=dev node scripts/register-webhooks.js",
    "webhooks:prod": "npx cross-env SHOPIFY_ENV=prod node scripts/register-webhooks.js",
    "seed:dev": "npx cross-env SHOPIFY_ENV=dev node scripts/seed-products.js",
    "seed:prod": "cross-env SHOPIFY_ENV=prod node scripts/seed-products.js",
    "abandoned:dev": "npx cross-env SHOPIFY_ENV=dev node scripts/abandoned-checkouts.js",
    "abandoned:prod": "npx cross-env SHOPIFY_ENV=prod node scripts/abandoned-checkouts.js",
    "postventa:dev": "npx cross-env SHOPIFY_ENV=dev node scripts/post-sale.js",
    "postventa:prod": "cross-env SHOPIFY_ENV=prod node scripts/post-sale.js",
    "abandoned:test": "node scripts/abandoned-checkouts.js --dry-run",
    "abandoned:run": "node scripts/abandoned-checkouts.js",
    "shorts:generate": "node scripts/generate-shorts.js",
    "orchestrate:deploy-product": "node scripts/orquestador.js publish-product ops/products-to-upload/cama-ansiedad-mascotas.md",
    "publish:plan": "node scripts/multi-channel-publisher.js",
    "audit:store": "node scripts/store-audit.js",
    "verify:prompts": "node scripts/verify_prompts.js",
    "audit:prompts": "node scripts/audit_prompts.js",
    "autofix": "node autoFixer.js",
    "elite:audit": "node agents/store-perfection-master.js",
    "elite:descriptions": "node agents/product-description-writer.js --auto-fix",
    "elite:images": "node agents/image-optimizer.js --auto-fix",
    "elite:trust": "node agents/trust-builder.js --auto-generate",
    "elite:fix-all": "node agents/store-auto-fixer.js",
    "knowledge:api": "node knowledge-api.js",
    "knowledge:import": "node agents/knowledge-importer.js",
    "knowledge:list": "node agents/knowledge-importer.js list"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.12.2",
    "commander": "^12.0.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "graphql-request": "^7.2.0",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.1.10",
    "papaparse": "^5.4.1",
    "sharp": "^0.34.4",
    "slugify": "^1.6.6",
    "unsplash-js": "^7.0.20",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}
